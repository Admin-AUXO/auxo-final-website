{
  "name": "auxo-homepage",
  "type": "module",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.19.1 <25.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "astro dev --port 4340",
    "build": "astro build",
    "build:fast": "NODE_OPTIONS=\"--max-old-space-size=4096\" astro build",
    "build:check": "npm run lint && npm run build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/",
    "preview": "astro preview --port 4341",
    "type-check": "tsc --noEmit",
    "lint:css": "stylelint \"src/styles/**/*.css\" --allow-empty-input",
    "lint:css:fix": "stylelint \"src/styles/**/*.css\" --fix --allow-empty-input",
    "lint": "npm run type-check && npm run lint:css",
    "lint:fix": "npm run lint:css:fix",
    "validate": "npm run lint && npm run build:check",
    "clean": "node scripts/clean.js",
    "clean:temp": "node scripts/clean-temp.js",
    "clean:all": "npm run clean",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:ci": "playwright test",
    "sanity:backup": "./scripts/export-sanity-data.sh",
    "sanity:restore": "npx sanity dataset import",
    "sanity:typegen": "npx sanity schema extract && npx sanity typegen generate",
    "sanity:schema": "node scripts/generate-schema.js",
    "sanity:deploy": "node scripts/deploy-schema.js",
    "sanity:migrate": "node scripts/migrate-data.js",
    "gtm:check": "npx tsx scripts/gtm-check.ts",
    "gtm:optimize": "npx tsx scripts/gtm-optimize.ts",
    "gtm:cleanup-vars": "npx tsx scripts/gtm-cleanup-variables.ts"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^6.0.2",
    "@emailjs/browser": "^4.4.1",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/simple-icons": "^1.2.63",
    "@sanity/astro": "^3.2.11",
    "@sanity/client": "^7.13.2",
    "@sanity/image-url": "^2.0.2",
    "astro": "^5.14.6",
    "astro-icon": "^1.1.5",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "googleapis": "^170.0.0",
    "lenis": "^1.2.11",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@sanity/cli": "^5.0.1",
    "@sanity/vision": "^5.1.0",
    "astro-edge": "^0.9.3",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.1.2",
    "playwright": "^1.57.0",
    "postcss-preset-env": "^10.5.0",
    "sharp": "^0.34.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1",
    "tsx": "^4.21.0",
    "tweetsodium": "^0.0.4",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "sharp": "^0.34.5",
    "prismjs": "^1.30.0"
  }
}
