---
import CtaSection from "../common/CtaSection.astro";
import { servicesContent } from "../../../data/content/services";

const content = servicesContent.cta;
---

<CtaSection
  id="services-cta"
  title={content.title}
  titleHighlight={["Analytics Roadmap", "Reality"]}
  description={content.description}
  descriptionHighlight={content.descriptionHighlight}
  ctaText={content.ctaText}
  ctaHref={content.ctaHref}
  variant="standard"
  background="default"
  maxWidth="4xl"
  class="services-cta-section"
/>
