---
import CtaSection from "../common/CtaSection.astro";
import { aboutContent } from "../../../data/content/about";

const content = aboutContent.cta;
---

<CtaSection
  id="about-cta"
  title={content.title}
  titleHighlight="Partnership"
  description={content.description}
  descriptionHighlight={content.descriptionHighlight}
  ctaText={content.ctaText}
  ctaHref={content.ctaHref}
  variant="standard"
  background="default"
  maxWidth="4xl"
  class="about-cta-section"
/>
